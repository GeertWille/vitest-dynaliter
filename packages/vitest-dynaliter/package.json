{
  "name": "vitest-dynaliter",
  "version": "0.0.1",
  "description": "Run your tests using Vitest & Dynalite",
  "license": "MIT",
  "repository": "https://github.com/geertwille/vitest-dynalite",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": {
    "name": "Geert Wille"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "pnpm build && eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "build": "rm -rf dist && tsup",
    "postinstallDev": "pnpm build && husky install",
    "prepack": "pnpm build",
    "prepare": "pnpm build"
  },
  "exports": {
    ".": "./dist/index.js",
    "./setupHooks": "./dist/setupHooks.js",
    "./environmentHooks": "./dist/environmentHooks.js"
  },
  "keywords": [
    "vitest",
    "dynamodb",
    "dynamodb local",
    "dynalite",
    "vitest preset",
    "vitest environment"
  ],
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "^3.645.0",
    "dynalite": "^3.2.2",
    "setimmediate": "^1.0.5"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": ">=3",
    "@aws-sdk/lib-dynamodb": ">=3",
    "vitest": ">=2.0.5"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.645.0",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitest/coverage-v8": "^2.0.5",
    "commitlint": "^19.4.1",
    "esbuild-node-externals": "^1.13.1",
    "eslint": "^9.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "tsc-files": "^1.1.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "**/*.ts": "tsc-files --noEmit",
    "*": "prettier --write --ignore-unknown"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.1.4"
}
